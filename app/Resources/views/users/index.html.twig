{% extends 'base.html.twig' %}

{% block body %}
    <table>
        <tr>
            <td>ID</td><td>Username</td><td>Email</td>
        </tr>
        {% for user in users %}
            <tr>
                <td><a href="{{ path('users_edit', {'id': user.id}) }}">{{ user.id }}</a></td>
                <td>{{ user.username }}</td>
                <td>{{ user.email }}
            </tr>
        {% endfor %}
    </table>
{% endblock %}